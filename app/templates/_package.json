{
  "name": "<%= libName %>",
  "version": "0.0.1",
  "description": "<%= libDescription %>",
  "directories": {
    "lib": "./lib"
  },
  "main": "lib/<%= baseFileName %>.js",
  "repository": {
    "type": "git",
    "url": "<%= githubUrl %>/<%= libName %>"
  },
  "keywords": [
    "es6",
    "micro",
    "microjs",
    "<%= libName %>"
  ],
  "author": {
    "name": "<%= realname %>",
    "email": "<%= email %>",
    "url": "<%= githubUrl %>"
  },
  "license": "MIT",
  "homepage": "<%= githubUrl %>/<%= libName %>",
  "scripts": {
    "watch": "gulp watch",
    "prepublish": "gulp prepublish",
    "test": "gulp test",
    "test-node": "gulp test-node",
    "test-browser": "gulp test-browser"
  },
  "babel": {
    "presets": [
      "es2015"
    ]
  },
  "dependencies": {
    "es6-lib": "^0.1.1",
    "babel-polyfill": "git://github.com/grammarly/babel-polyfill.git#1.0.1",
    "chai": "^3.4.0"
  }
}
